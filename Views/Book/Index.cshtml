@model System.Data.DataTable
@{
    ViewBag.Title = "Index";
}

<table class="table table-bordered table-striped">
    <tr>
        <th>Book</th>
        <th>Author</th>
        <th>Published</th>
        <th>Available</th>
        <th></th>
    </tr>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
<tr>
    <td>@Model.Rows[i][1]</td>
    <td>@Model.Rows[i][2]</td>
    <td>@Model.Rows[i][3]</td>
    <td>@Model.Rows[i][4]</td>
    <td>
        <a href="@Url.Action("Edit", "Book", new {@id=@Model.Rows[i][0] })">Edit</a>
        <a href="@Url.Action("Delete", "Book", new {@id=@Model.Rows[i][0] })">Delete</a>
    </td>
</tr>
    }
</table>

<a href="@Url.Action("Create", "Book")">Add Book</a>